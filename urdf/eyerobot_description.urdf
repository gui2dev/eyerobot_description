<robot name="eyerobot">

<gazebo>
 <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
 <robotNamespace>/eyerobot</robotNamespace>
 <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
 </plugin>
 <plugin name="imu_plugin" filename="libgazebo_ros_imu.so">
   <alwaysOn>true</alwaysOn>
   <bodyName>base_link</bodyName>
   <topicName>imu_base</topicName>
   <serviceName>imu_base_service</serviceName>
   <gaussianNoise>0.0</gaussianNoise>
   <updateRate>20.0</updateRate>
 </plugin>
 <plugin name="imu_plugin" filename="libgazebo_ros_imu.so">
   <alwaysOn>true</alwaysOn>
   <bodyName>gimbal_roll</bodyName>
   <topicName>imu_gimbal</topicName>
   <serviceName>imu_gimbal_service</serviceName>
   <gaussianNoise>0.0</gaussianNoise>
   <updateRate>20.0</updateRate>
 </plugin>

  <plugin name="skid_steer_drive_controller" filename="libgazebo_ros_skid_steer_drive.so">
    <updateRate>100.0</updateRate>
    <robotNamespace>/eyerobot</robotNamespace>
    <leftFrontJoint>motor_to_rim_fl</leftFrontJoint>
    <rightFrontJoint>motor_to_rim_fr</rightFrontJoint>
    <leftRearJoint>motor_to_rim_rl</leftRearJoint>
    <rightRearJoint>motor_to_rim_rr</rightRearJoint>
    <wheelSeparation>0.300</wheelSeparation>
    <wheelDiameter>0.120</wheelDiameter>
    <robotBaseFrame>base_link</robotBaseFrame>
    <torque>20</torque>
    <topicName>cmd_vel</topicName>
    <broadcastTF>false</broadcastTF>
  </plugin>

</gazebo>

